[user]
  name = Juan Raya
  email = jraya@commercehub.com
  username = JuanRaya07
[alias]
  s = status
  a = add --all
  l = log --oneline
  lg = log --oneline --graph --decorate
  pl = pull
  cl = clone
  ai = add -i
  m = merge
  r = restore
  b = branch
  c = commit
  ca = commit -a
  cm = commit -m
  f = fetch
  fo = fetch origin
  fu = fetch upstream
  o = checkout
  ps = push
  pso = push origin
  undo = reset --soft HEAD^
[init]
    defaultBranch = main
[color]
    ui = true
; [pull]
; 	rebase = true
[push]
	autoSetupRemote = true
[core]
    editor = nvim
    ignorecase = false
	pager = delta
	excludesfile = /Users/jraya/.gitignore_global
[delta]
    navigate = true
	line-numbers = true
	dark = true
    syntax-theme = gruvbox-dark
[diff]
    colorMoved = default
