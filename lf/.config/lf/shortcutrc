map gh cd ~/
map gd cd ~/Downloads
map go cd ~/Documents
map gp cd ~/Pictures
map gv cd ~/Videos
map gm cd ~/Music 

map gc cd ~/code
map gw cd ~/code/workspace
map g. cd ~/dotfiles
map gx cd ~/Dropbox

map gC cd ~/.config
map gn cd ~/.config/nvim
map gz cd ~/.config/zsh
map gf cd ~/.config/lf
map gs cd ~/.config/shell

map gb cd "~/.local/bin" 
map glsr cd "~/.local/src" 
map glsh cd "~/.local/share" 

map g/ cd /
map gE cd /etc
map gB cd /boot
map gM cd /mnt
map gU cd /usr
map gO cd /opt
map gL cd /usr/lib
map gB cd /usr/bin
map gV cd /var

map gta $$EDITOR "$HOME/.config/shell/aliasrc" 
map gtp $$EDITOR "$HOME/.config/shell/profile" 
map gtn $$EDITOR "$HOME/.config/nvim/init.lua" 
map gtz $$EDITOR "$HOME/.config/zsh/.zshrc" 
map gtf $$EDITOR "$HOME/.config/lf/lfrc" 
map gts $$EDITOR "$HOME/.config/lf/shortcutrc" 
